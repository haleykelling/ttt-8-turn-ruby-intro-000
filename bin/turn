#!/usr/bin/env ruby
require_relative '../lib/turn.rb'

puts "Welcome to Tic Tac Toe!"

board=[" ", " ", " ", " ", " ", " ", " ", " ", " "]

display_board(board)

puts "Where would you like to make your first move?"
turn
input = gets.strip

move(board, input_to_index(input), "X")

display_board(board)
